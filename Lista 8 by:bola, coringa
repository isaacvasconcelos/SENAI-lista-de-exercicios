I, RESULT: INTEIRO
RESP, PESQ: CARACTER
inicio
// Seção de Comandos
 para i de 1 ate 10 passo 1 faca
 escreval ("Informe seu nome:")
 leia(nome[i])
 fimpara
 Limpatela
 repita
 escreval ("Deseja fazer pesquisa de nome (S/N) ?")
 leia (resp)
 se resp = "S" entao
 Escreval ("Informe o nome a ser pesquisado")
 leia (pesq)
 result:=0
 para i de 1 ate 10 passo 1 faca
 se pesq = nome[i] entao
 Result := result +1
 fimse

algoritmo ""
// Função :
// Autor : 
// Data : 27/09/2016
// Seção de Declarações
var
vetA: vetor [1..20] de inteiro
vetB : vetor [1..20] de real
I, J : inteiro
Fat: real
inicio
// Seção de Comandos
Para I de 1 ate 20 passo 1 faca
escreval ("escreva um numero a ser calculado o fatorial")
leia (vetA[I])
Fat:= 1
Para J de 1 ate 20 passo 1 faca
Fat:= Fat * J
Fimpara
vetB [I] := Fat
escreval (vetA [I], "! =", vetB[I])
escreval ()
fimpara
fimalgoritmo

algoritmo "semnome"
// Função :
// Autor :
// Data : 29/09/2016
// Seção de Declarações 
var
 I:Inteiro
 nome,sit:vetor[1..15] de caracter
 pr1,pr2,media:vetor[1..15] de real
inicio
para I de 1 ate 15 passo 1 faca
escreval("Informe o nome do aluno",I)
leia(NOME[I])
Escreval("Informe a nota da PR1 do aluno ",nome[I])
Leia(PR1[I]).
Escreval("Informea nota da PR2 do Aluno ",NOME[I])
Leia(PR2[I])
MEDIA[I]<-(PR1[I] + PR2[I])/2
Se MEDIA [I]>=5 entao
SIT[I]<-"AP"
senao
SIT[I]<-"RP"
fimse
escreval
fimpara
Escreval("Relatorio final sobre a situaçao dos alunos")
escreval
para I de 1 ate 15 passo 1 faca
Escreval("ALUNO",1":!,NOME[i])
Escreval("NOTA PR1:",PR1[I])
Escreval("NOTA PR2:",PR2[I])
Escreval("MEDIA",MEDIA[I])
Escreval("SITUAÇAO: ",SIT[I])
ESCREVAL
FIMPARA
// Seção de Comandos 
fimalgoritmo

algoritmo ""
// Função :
// Autor :
// Data : 29/09/2016
// Seção de Declarações 
var
i : inteiro
nome : vetor [1..20] de caractere
sal, novosal : vetor [1..20] de real
inicio
// Seção de Comandos 
para i de 1 ate 20 passo 1 faca
     escreval ("Informe o nome do funcionário:")
     leia (nome[i])
     escreval ("Infirne o salário do funcionário:")
     leia (sal[i])
     novosal[i] <- sal[i] * 1.08 //acrescimo de 8% no salário
fimpara
limpatela
escreval ("Lista com o nome dos funcionários e seus salário reajustados em 8%:")
escreval (   )      // ou escreval
para i de 1 ate 20 passo 1 faca
escreval ("Funcionário ", i)
escreval ("Nome ", nome[i])
escreval ("Novo Salário: ", novosal[i])
escreval
        fimpara

fimalgoritmo
